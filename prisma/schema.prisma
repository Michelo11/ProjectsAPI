generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Project {
  id          Int     @id @default(autoincrement())
  name        String
  description String
  images      Image[]
}

model Image {
  id        Int     @id @default(autoincrement())
  name      String
  project   Project @relation(fields: [projectId], references: [id], onDelete: Cascade)
  projectId Int
  path      String
}
